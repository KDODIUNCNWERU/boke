            "languageId": null
        },
        {
            "stepNo": 5,
            "content": "把甜豉油材料勻合",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7175_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 6,
            "content": "把油塗在蒸盤上",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7175_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 7,
            "content": "然後倒粉漿在蒸盤中",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7176_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 8,
            "content": "在鑊中加水隔水蒸23分鐘",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7176_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 9,
            "content": "加上牛肉芽菜馅料然后捲起",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7177_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 10,
            "content": "加上牛肉芽菜馅料然后捲起",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7177_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 11,
            "content": "再隔水用大火蒸2分钟",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_7178_item_0.jpg",
            "languageId": null
        },
        {

    "steps": [
        {
            "stepNo": 1,
            "content": "將洋蔥切片",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27571_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 2,
            "content": "將番茄切片",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27571_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 3,
            "content": "將茄子切片",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27581_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 4,
            "content": "將青瓜切片",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27581_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 5,
            "content": "準備醬汁，將蒜、松子、番茄碎和九層塔放入攪拌機，加入橄欖油",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27591_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 6,
            "content": "打至幼滑",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27591_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 7,
            "content": "在焗盤裡擺好燈籠椒、洋蔥、茄子、青瓜和番茄。撒上橄欖油和鹽",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27601_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 8,
            "content": "放入預熱至220度的焗爐焗10分鐘",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27601_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 9,
            "content": "在一片千層麵上放一湯匙醬汁，和烤蔬菜一起上桌",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27611_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 10,
            "content": "最宜於室溫下作為前菜食用",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_27611_item_1.jpg",
            "languageId": null
        }

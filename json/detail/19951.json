    "steps": [
        {
            "stepNo": 1,
            "content": "把蔥切段",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39121_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 2,
            "content": "沖洗乾淨黃花魚後，抹乾魚身，用刀割魚身數次",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39121_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 3,
            "content": "混合所有醬汁材料，備用",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39131_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 4,
            "content": "將魚撲上粟粉",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39131_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 5,
            "content": "在鑊中加入花生油，以中高火加熱，炸魚至每邊金黃",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39141_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 6,
            "content": "把魚放置於廚房紙巾上，吸去多餘油",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39141_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 7,
            "content": "在鑊中加入1湯匙油，以中火燒熱，並炒蔥及加入醬汁",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39151_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 8,
            "content": "加入100毫升清水及粟粉水（用2湯匙水混合1湯匙粟粉），並將醬汁煮至濃稠",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_39151_item_1.jpg",
            "languageId": null
        }

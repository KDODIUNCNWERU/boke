    "steps": [
        {
            "stepNo": 1,
            "content": "食材",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12021_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 2,
            "content": "臘腸切成細條",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12021_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 3,
            "content": "在大碗中將麵粉，泡打粉，酵母，糖，鹽，油和水混合",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12041_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 4,
            "content": "用濕布或保鮮紙蓋住麵團，直到麵團發酵至原本的2倍大",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12041_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 5,
            "content": "將麵團切成8-10份",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12051_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 6,
            "content": "每塊麵團揉成長條",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12051_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 7,
            "content": "繞在臘腸上",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12061_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 8,
            "content": "然後放在刷上一層油的蒸盤上",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12061_item_1.jpg",
            "languageId": null
        },
        {
            "stepNo": 9,
            "content": "隔水高火蒸10分鐘",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12071_item_0.jpg",
            "languageId": null
        },
        {
            "stepNo": 10,
            "content": "即成",
            "imageUrl": "http://pic.daydaycook.com/production/ImageMigration/384x288/recipe_content_12071_item_1.jpg",
            "languageId": null
        }
